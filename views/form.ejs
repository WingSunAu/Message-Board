<h1>Create a New Post</h1>
<form method="POST" action="/new">
    <input name="id" value="<%=id %>" style=" position: absolute; visibility: hidden;">
    <input type="text" name="user" id="user<%=id %>" required>
    <input type="text" name="text" id="text<%=id %>" required>
    <button type="submit">Post</button>
</form>